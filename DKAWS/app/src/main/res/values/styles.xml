<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="appTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="main_tab_bottom">
        <item name="android:textSize">@dimen/bottom_tab_font_size</item>
        <item name="android:textColor">@drawable/selector_main_bottom_textcolor</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">@dimen/bottom_tab_padding_drawable</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="btn_registerandlogin">
        <item name="android:textColor">#fff</item>
        <item name="android:background">@drawable/shape_confirm</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
    </style>
    <!--文本内容的style-->
    <style name="text_content">
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">6dp</item>
    </style>

    <style name="item_complete_info">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">49dp</item>
        <item name="android:paddingLeft">17dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColorHint">@color/text_hint</item>
    </style>


    <style name="mine_item_leftimage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="mine_item_lefttext" parent="mine_item_leftimage">
        <item name="android:textColor">@color/text_gray1</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="mine_item_rightimage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">15dp</item>
    </style>

    <style name="mine_evaluate_satisfaction_txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textColor">@color/text_mine_service</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="mine_evaluate_people_txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_mine_service</item>
    </style>

    <style name="aboutus_lefttext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_common</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="aboutus_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">#fff</item>
        <item name="android:paddingLeft">17dp</item>
        <item name="android:paddingRight">13dp</item>
    </style>

    <style name="aboutus_righttext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/theme_color</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="setting_rightimage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">13dp</item>
    </style>

    <style name="setting_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingLeft">13dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <!-- <item name="android:paddingRight">13dp</item>-->

    <style name="setting_commentquestion_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingLeft">17dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/white</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/text_common</item>

    </style>

    <style name="setting_commentquestion_arrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@drawable/icon_mine_arrow</item>
        <item name="android:layout_marginRight">15dp</item>
    </style>

    <style name="setting_commentquestion_text_expand">

        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingLeft">17dp</item>
        <item name="android:paddingRight">17dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/tip_color</item>
    </style>

    <style name="myinfo_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/main_content</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="myinfo_righttext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">15sp</item>

    </style>

    <style name="advice_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:paddingLeft">13dp</item>
        <item name="android:textColorHint">@color/txt_hint</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="invitefriend_share_btn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textColor">@color/tip_color</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="translucent" parent="@android:style/Animation">   
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="select_phptp_activity" parent="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">
        <item name="android:windowAnimationStyle">@style/translucent</item>
    </style>

    <style name="apply_kind">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#585858</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="apply_kind_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">13dp</item>
    </style>

    <style name="apply_diseasekind">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">29dp</item>
        <item name="android:textColor">@color/theme_color</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="mime_myincome_takemoney">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="cutlint">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/cutline</item>
    </style>

    <style name="appointdetail_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">3</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">#f8f8f8</item>
    </style>

    <style name="appointdetail_text1">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/main_content</item>
        <item name="android:layout_height">44dp</item>
    </style>

    <style name="appointdetail_text2">
        <item name="android:layout_weight">2</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/txt_hint_color</item>
        <item name="android:layout_height">44dp</item>
    </style>

    <style name="verticality">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/cutline</item>
    </style>


    <style name="popupwindow_style">
        <item name="android:windowEnterAnimation">@anim/show_popupwindow</item>
        <item name="android:windowExitAnimation">@anim/hide_popupwindow</item>
    </style>
</resources>
